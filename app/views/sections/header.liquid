---
{
  "name": "Header",
  "class": "",
  "icon": "header",
  "keep_name": true,
  "keep_icon": true,
  "id": "header",
  "settings": [
    {
      "label": "Fixed header",
      "id": "fixed_position",
      "type": "checkbox"
    },
    {
      "label": "Brand Logo",
      "id": "brand_logo",
      "type": "image_picker"
    }
  ],
  "default": {
    "settings": {
      "brand_logo": "/samples/logo-negatif.png"
    }
  }
}
---
<nav class="navbar navbar-expand-lg navbar-light">
  <div class="container">
    <div class="row pt-2 pb-2 align-items-center w-100">
      <div class="col-auto d-flex">
        <a class="navbar-brand" href="#">
          <img src="{{section.settings.brand_logo}}" alt="{{site.metafields.name}}" class="img-fluid">
        </a>
      </div>
    </div>
  </div>
</nav>
